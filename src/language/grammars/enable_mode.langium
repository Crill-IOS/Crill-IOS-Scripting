import 'common'

//child Modes
import 'configure_mode'


//Start--------------------------------
Enable_cmds :
    (
        NL
        |Configure_cmd
        |Show_cmd
        |Ping_cmd
    );



Ping_cmd:
    'ping' (hostname=[Hostname_Input] | ip=IP) NL
    ;

Show_cmd:
    'show' options=Show_cmd_options 
    ;

Show_cmd_options:
    option=
        'run' NL
        | 'interface' NL
    ;

Configure_cmd: 
    'configure' Configure_cmd_options
    ;

Configure_cmd_options:
        'terminal' NL lines+=Configure_cmds+ 
        |'memory' NL
    ;
